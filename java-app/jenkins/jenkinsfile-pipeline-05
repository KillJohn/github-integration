pipeline {
  agent {
     node { 
        label 'node01'
        options { timestamps() }

     } 
  }

  tools {
   maven 'maven-apache-3.8.4'
 }
 
stages {
 stage('Maven Package') {
   steps {
     dir('java-app'){
       sh 'mvn package'
     }
   }
 }
 stage('Run') {
   steps {
      cleanWs()
   }
 }
}
stage('build'){
  steps {
    sh 'touch `date`'
  }
}
stage('validation') {

}
stage('deploy') {
  steps {
    sh 'echo $JAVA'
  }
}

}